// Fibonacci-Funktion für Export
GIBFREI GANZ fibonacci(n: GANZ) {
  WENN (n < 2) {
    ZURÜCK n;
  }
  ZURÜCK fibonacci(n-1) + fibonacci(n-2);
}

// Lokale Version für interne Tests
GANZ fib(n: GANZ) {
  WENN (n < 2) {
    ZURÜCK n;
  }
  ZURÜCK fib(n-1) + fib(n-2);
}

GANZ haupt() {
  DRUCKE("Fibonacci(10):");
  DRUCKE(fib(10));
  ZURÜCK 0;
}
